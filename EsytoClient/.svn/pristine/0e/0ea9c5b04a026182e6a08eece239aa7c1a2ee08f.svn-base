<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:client="http://schemas.android.com/apk/res/com.ec2.yspay"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" 
    android:background="@color/gray_bg_color">
	<com.ec2.yspay.widget.MyTitle 
        android:id="@+id/rl_top"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        client:isShowLeft="true"
        client:titleName="支付成功"/>
	<RelativeLayout 
	    android:layout_width="fill_parent"
	    android:layout_height="100dp">
	    <LinearLayout 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:orientation="horizontal"
	        android:gravity="center_vertical"
	        android:layout_centerInParent="true">
	        <ImageView 
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:src="@drawable/finished_icon_72_72"/>
	        <TextView style="@style/textview_green_style"
	            android:layout_marginLeft="7dp"
	            android:text="支付成功"/>
	    </LinearLayout>
	</RelativeLayout>
	<View 
	    style="@style/line_view_gray"/>
	<LinearLayout
        android:id="@+id/ll_yingshou"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:padding="@dimen/margin_left"
        android:background="@color/white"
        android:orientation="vertical">
	    <RelativeLayout 
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content">
	        <TextView 
		        style="@style/textview_black2"
		        android:layout_centerVertical="true"
		        android:text="应收金额"/>
		    <TextView 
		        android:id="@+id/tv_yingshou"
		        style="@style/textview_black1"
		        android:layout_alignParentRight="true"
		        android:layout_centerVertical="true"
		        android:textSize="32dp"
		        android:text="￥123.10"/>
	    </RelativeLayout>
	    <RelativeLayout 
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
	        android:layout_marginTop="12dp">
	        <TextView 
		        style="@style/textview_black2"
		        android:layout_centerVertical="true"
		        android:text="实收金额"/>
		    <TextView 
		        android:id="@+id/tv_shishou"
		        style="@style/textview_black1"
		        android:layout_alignParentRight="true"
		        android:layout_centerVertical="true"
		        android:textSize="18dp"
		        android:text="￥123.11"/>
	    </RelativeLayout>
	    <RelativeLayout 
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
	        android:layout_marginTop="12dp">
	        <TextView 
		        style="@style/textview_black2"
		        android:layout_centerVertical="true"
		        android:text="找零金额"/>
		    <TextView 
		        android:id="@+id/tv_zhaoling"
		        style="@style/textview_black1"
		        android:layout_alignParentRight="true"
		        android:layout_centerVertical="true"
		        android:textSize="18dp"
		        android:text="￥123.11"/>
	    </RelativeLayout>
	</LinearLayout>
	<View 
	    style="@style/line_view_gray"/>
    
	<Button 
	    style="@style/btn_red636_style"
	    android:layout_marginTop="40dp"
	    android:onClick="onclick_back"
	    android:text="完成"/>
	<com.ec2.yspay.widget.ButtonWhiteCenter 
	        android:id="@+id/btn_print"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_gravity="center_horizontal"
	        android:layout_marginTop="14dp"
	        client:btnContent="打印"/>
</LinearLayout>
