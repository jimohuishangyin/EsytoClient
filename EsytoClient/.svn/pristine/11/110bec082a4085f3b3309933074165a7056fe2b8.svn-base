package com.ec2.yspay.http.cash;

import java.io.Serializable;

/**
 * <一句话功能简述>
 * <功能详细描述>
 * 
 * @author   罗洪祥
 * @version  V001Z0001
 * @date     2015年10月28日
 * @see  [相关类/方法]
 * @since  [产品/模块版本]
 */
public class ShopItem implements Serializable
{
    private String shopName;
    private String shopCode;
    private String address;
    private String city;
    private String logoImage;
    private String logoImageUrl;
    private String phone;
    private String province;
    /**
     * 获取 shopName
     * @return 返回 shopName
     */
    public String getShopName()
    {
        return shopName;
    }

    /**
     * 设置 shopName
     * @param 对shopName进行赋值
     */
    public void setShopName(String shopName)
    {
        this.shopName = shopName;
    }

    /**
     * 获取 shopCode
     * @return 返回 shopCode
     */
    public String getShopCode()
    {
        return shopCode;
    }

    /**
     * 设置 shopCode
     * @param 对shopCode进行赋值
     */
    public void setShopCode(String shopCode)
    {
        this.shopCode = shopCode;
    }

    /**
     * 获取 address
     * @return 返回 address
     */
    public String getAddress()
    {
        return address;
    }

    /**
     * 设置 address
     * @param 对address进行赋值
     */
    public void setAddress(String address)
    {
        this.address = address;
    }

    /**
     * 获取 city
     * @return 返回 city
     */
    public String getCity()
    {
        return city;
    }

    /**
     * 设置 city
     * @param 对city进行赋值
     */
    public void setCity(String city)
    {
        this.city = city;
    }

    /**
     * 获取 logoImage
     * @return 返回 logoImage
     */
    public String getLogoImage()
    {
        return logoImage;
    }

    /**
     * 设置 logoImage
     * @param 对logoImage进行赋值
     */
    public void setLogoImage(String logoImage)
    {
        this.logoImage = logoImage;
    }

    /**
     * 获取 phone
     * @return 返回 phone
     */
    public String getPhone()
    {
        return phone;
    }

    /**
     * 设置 phone
     * @param 对phone进行赋值
     */
    public void setPhone(String phone)
    {
        this.phone = phone;
    }

    /**
     * 获取 province
     * @return 返回 province
     */
    public String getProvince()
    {
        return province;
    }

    /**
     * 设置 province
     * @param 对province进行赋值
     */
    public void setProvince(String province)
    {
        this.province = province;
    }

    /** 
     * <默认构造函数>
     */
    public ShopItem(String shopName, String shopCode)
    {
        super();
        this.shopName = shopName;
        this.shopCode = shopCode;
    }

    /** 
     * <默认构造函数>
     */
    public ShopItem(String shopName, String shopCode, String address, String city, String logoImage, String phone,
        String province)
    {
        super();
        this.shopName = shopName;
        this.shopCode = shopCode;
        this.address = address;
        this.city = city;
        this.logoImage = logoImage;
        this.phone = phone;
        this.province = province;
    }

    /**
     * 获取 logoImageUrl
     * @return 返回 logoImageUrl
     */
    public String getLogoImageUrl()
    {
        return logoImageUrl;
    }

    /**
     * 设置 logoImageUrl
     * @param 对logoImageUrl进行赋值
     */
    public void setLogoImageUrl(String logoImageUrl)
    {
        this.logoImageUrl = logoImageUrl;
    }

    /** 
     * <默认构造函数>
     */
    public ShopItem(String shopName, String shopCode, String address, String city, String logoImage,
        String logoImageUrl, String phone, String province)
    {
        super();
        this.shopName = shopName;
        this.shopCode = shopCode;
        this.address = address;
        this.city = city;
        this.logoImage = logoImage;
        this.logoImageUrl = logoImageUrl;
        this.phone = phone;
        this.province = province;
    }

    
    
}
