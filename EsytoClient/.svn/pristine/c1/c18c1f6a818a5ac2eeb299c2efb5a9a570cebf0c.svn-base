<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:client="http://schemas.android.com/apk/res/com.ec2.yspay"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
	<com.ec2.yspay.widget.MyTitle 
        android:id="@+id/rl_top"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        client:isShowLeft="true"
        client:titleName="打印"/>
	<LinearLayout 
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:orientation="vertical"
	    android:padding="@dimen/padding_parent"
	    android:background="@color/white">
	    <com.ec2.yspay.widget.PrintLinearLayoutButton 
	        android:id="@+id/llbtn_print_last"
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
	        android:layout_marginTop="25dp"
	        client:btnContent="打印最后一笔"/>
	    <com.ec2.yspay.widget.PrintLinearLayoutButton 
	        android:id="@+id/llbtn_print_one"
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
	        client:btnContent="打印指定一笔"/>
	    <com.ec2.yspay.widget.PrintLinearLayoutButton 
	        android:id="@+id/llbtn_print_list"
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
	        client:btnContent="查询当天订单列表打印"/>
	    <com.ec2.yspay.widget.PrintLinearLayoutButton 
	        android:id="@+id/llbtn_print_all"
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
	        client:btnContent="当日汇总打印"/>
	    <com.ec2.yspay.widget.PrintLinearLayoutButton 
	        android:id="@+id/llbtn_print_detail"
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
	        client:btnContent="当日交易明细打印"/>
	</LinearLayout>
	
</LinearLayout>