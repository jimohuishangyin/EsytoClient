<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:client="http://schemas.android.com/apk/res/com.ec2.yspay"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
	<com.ec2.yspay.widget.MyTitle 
        android:id="@+id/rl_top"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        client:isShowLeft="true"
        client:titleName="撤销与退款"/>
	<LinearLayout 
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:orientation="vertical"
	    android:padding="@dimen/padding_parent"
	    android:background="@color/white">
	    <LinearLayout 
	        android:id="@+id/ll_ordermsg"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_below="@id/pay_title"
	        android:gravity="center_vertical"
	        android:orientation="horizontal"
	        android:layout_marginTop="20dp">
	        <LinearLayout 
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        
		        android:orientation="vertical">
		        <TextView 
		            style="@style/textview_gray"
		            android:text="交易号"/>
		        <TextView 
		            style="@style/textview_gray"
		            android:layout_marginTop="@dimen/textgray_margin"
		            android:text="凭证号"/>
		        <TextView 
		            style="@style/textview_gray"
		            android:layout_marginTop="@dimen/textgray_margin"
		            android:text="时间"/>
		        <TextView 
		            style="@style/textview_gray"
		            android:layout_marginTop="@dimen/textgray_margin"
		            android:text="支付方式"/>
		        <TextView 
		            style="@style/textview_gray"
		            android:layout_marginTop="@dimen/textgray_margin"
		            android:visibility="gone"
		            android:text="撤销方式"/>
		    </LinearLayout>
		    <LinearLayout 
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_marginLeft="15dp"
		        android:orientation="vertical">
		        <TextView 
		            android:id="@+id/tv_orderid"
		            style="@style/textview_gray"
		            android:text=""/>
		        <TextView 
		            android:id="@+id/tv_voucherId"
		            style="@style/textview_gray"
		            android:layout_marginTop="@dimen/textgray_margin"
		            android:text=""/>
		        <TextView 
		            android:id="@+id/tv_orderTime"
		            style="@style/textview_gray"
		            android:layout_marginTop="@dimen/textgray_margin"
		            android:text=""/>
		        <TextView 
		            android:id="@+id/tv_channelType"
		            style="@style/textview_gray"
		            android:layout_marginTop="@dimen/textgray_margin"
		            android:text=""/>
		        <TextView 
		            style="@style/textview_gray"
		            android:layout_marginTop="@dimen/textgray_margin"
		            android:visibility="gone"
		            android:text="1001"/>
		    </LinearLayout>
	    </LinearLayout>
	    <TextView 
	        android:id="@+id/tv_amount"
	        style="@style/textview_black"
	        android:layout_marginTop="25dp"
	        android:text="退款金额        ￥0.00"/>
	    <TextView 
	        style="@style/textview_black"
	        android:textSize="30dp"
	        android:layout_gravity="center_horizontal"
	        android:layout_marginTop="40dp"
	        android:textColor="@color/text_pink"
	        android:text="退款成功！"/>
	
	<Button 
	    style="@style/btn_red636_style"
	    android:layout_marginTop="40dp"
	    android:onClick="onclick_back"
	    android:text="完成"/>
	<com.ec2.yspay.widget.ButtonWhiteCenter 
	        android:id="@+id/btn_print"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_gravity="center_horizontal"
	        android:layout_marginTop="10dp"
	        client:btnContent="打印"/>
	</LinearLayout>
	
</LinearLayout>