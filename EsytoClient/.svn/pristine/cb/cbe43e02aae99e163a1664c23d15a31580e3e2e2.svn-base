<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:client="http://schemas.android.com/apk/res/com.ec2.yspay"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" 
    android:background="@color/white">
	<com.ec2.yspay.widget.MyTitle 
        android:id="@+id/rl_top"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        client:isShowLeft="true"
        client:titleName="打印"/>
	<RelativeLayout 
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:background="#e3e3e3"
	    android:paddingTop="10dp"
	    android:paddingBottom="10dp"
	    android:paddingLeft="@dimen/margin_left"
	    android:paddingRight="@dimen/margin_left">
	    <TextView 
	        android:id="@+id/tv_year"
	        style="@style/textview_gray"
            android:text="2015"/>
	    <LinearLayout 
	        android:id="@+id/ll_date"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_below="@id/tv_year"
	        android:orientation="horizontal">
	        <TextView 
	           android:id="@+id/tv_month"
	           style="@style/textview_black"
	           android:textSize="20dp"
	           android:text="09-21"/>
	        
	    </LinearLayout>
        <TextView 
            android:id="@+id/tv_operator"
            style="@style/textview_gray"
            android:text="罗洪祥"
            android:textSize="16dp"
            android:layout_alignParentRight="true"/>
        <ImageView 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toLeftOf="@id/tv_operator"
            android:layout_marginRight="12dp"
            android:layout_marginTop="2dp"
            android:src="@drawable/cashier48_48"/>
        <TextView 
            android:id="@+id/tv_time"
            style="@style/textview_black"
            android:layout_below="@id/tv_operator"
            android:layout_marginTop="4dp"
            android:layout_alignParentRight="true"
            android:text="00:00:00-12:12:00"/>
	</RelativeLayout>
	<RelativeLayout 
	    android:layout_width="fill_parent"
	    android:layout_height="60dp"
	    android:paddingLeft="@dimen/margin_left"
	    android:paddingRight="@dimen/margin_left"
	    android:paddingTop="5dp"
	    android:paddingBottom="2dp">
	    <TextView 
	        android:id="@+id/tv_zmmx"
	        style="@style/textview_black"
	        android:textSize="14dp"
	        android:layout_alignParentBottom="true"
	        android:layout_marginBottom="10dp"
            android:text="252笔"/>
        <ImageView 
            android:layout_width="0.4dp"
            android:layout_height="40dp"
            android:layout_toRightOf="@id/tv_zmmx"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="20dp"
            android:src="@color/line_gray"/>
        <LinearLayout
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:layout_alignParentRight="true"
            android:gravity="bottom"
            android:layout_alignParentBottom="true"
	        android:layout_marginBottom="10dp"
            android:orientation="horizontal">
            <TextView style="@style/textview_gray"
                android:textSize="16dp"
                android:text="总金额："/>
            <TextView 
                android:id="@+id/tv_totalMoney"
                style="@style/textview_pink"
                android:layout_marginLeft="8dp"
                android:textSize="20dp"
                android:text="25623.21元"/>
        </LinearLayout>
        
	</RelativeLayout>
	<ImageView 
	    android:layout_width="fill_parent"
	    android:layout_height="1dp"
	    android:src="@color/line_gray"/>
	<ImageView 
	    android:layout_width="fill_parent"
	    android:layout_height="10dp"
	    android:src="#e3e3e3"/>
	<ImageView 
	    android:layout_width="fill_parent"
	    android:layout_height="1dp"
	    android:src="@color/line_gray"/>
	<RelativeLayout 
	    android:layout_width="match_parent"
	    android:layout_height="match_parent">
        <RelativeLayout 
            android:id="@+id/rl_buttom"
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:layout_alignParentBottom="true">
            <ImageView 
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:src="@color/line_gray"/>
            <LinearLayout 
                android:layout_width="fill_parent"
                android:layout_height="60dp"
                android:orientation="horizontal"
                android:layout_centerVertical="true"
                android:gravity="center"
                android:layout_marginLeft="@dimen/margin_left"
                android:layout_marginRight="@dimen/margin_left">
                
                <com.ec2.yspay.widget.ButtonBlueCenterSmall
			        android:id="@+id/btn_sure"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        client:btnContent="打印"/>
            </LinearLayout>
        </RelativeLayout>
	    <com.ec2.yspay.widget.PullToRefreshLayout
	    	android:id="@+id/refresh_view"
		    android:layout_width="match_parent"
		    android:layout_height="match_parent" 
		   android:layout_above="@id/rl_buttom">
		    <include layout="@layout/refresh_head" />
		
		    <com.ec2.yspay.widget.PullableListView
		        android:id="@+id/content_view"
		        android:layout_width="match_parent"
		        android:layout_height="match_parent"
		        android:background="@color/white"
		        android:divider="#f5f5f5"
		        android:dividerHeight="1dp" />
		
		    <include layout="@layout/load_more" />
		
		</com.ec2.yspay.widget.PullToRefreshLayout>
	</RelativeLayout>
	 
	
</LinearLayout>