<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:client="http://schemas.android.com/apk/res/com.ec2.yspay"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
	<com.ec2.yspay.widget.MyTitle 
        android:id="@+id/rl_top"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        client:isShowLeft="true"
        client:titleName="打印"/>
	<LinearLayout 
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:orientation="vertical"
	    android:padding="@dimen/padding_parent"
	    android:background="@color/white">
	    <TextView 
	        android:id="@+id/tv_all_user"
	        style="@style/textview_black"
	        android:layout_marginTop="20dp"
	        android:text="收银员："/>
	    <TextView 
	        android:id="@+id/tv_all_begintime"
	        style="@style/textview_black"
	        android:layout_marginTop="@dimen/print_margin_textveiw"
	        android:text="开始时间："/>
	    <TextView 
	        android:id="@+id/tv_all_endtime"
	        style="@style/textview_black"
	        android:layout_marginTop="@dimen/print_margin_textveiw"
	        android:text="结束时间："/>
	    <TextView 
	        android:id="@+id/tv_all_cash"
	        style="@style/textview_black"
	        android:layout_marginTop="@dimen/print_margin_textveiw"
	        android:text="现金：0.00元（0笔）"/>
	    <TextView 
	        android:id="@+id/tv_all_ali"
	        style="@style/textview_black"
	        android:layout_marginTop="@dimen/print_margin_textveiw"
	        android:text="支付宝：0.00元（0笔）"/>
	    <TextView 
	        android:id="@+id/tv_all_best"
	        style="@style/textview_black"
	        android:layout_marginTop="@dimen/print_margin_textveiw"
	        android:text="翼支付：0.00元（0笔）"/>
	    <TextView 
	        android:id="@+id/tv_all_card"
	        style="@style/textview_black"
	        android:layout_marginTop="@dimen/print_margin_textveiw"
	        android:text="银行卡：0.00元（0笔）"/>
	    <TextView 
	        android:id="@+id/tv_all_wx"
	        style="@style/textview_black"
	        android:layout_marginTop="@dimen/print_margin_textveiw"
	        android:text="微信支付：0.00元（0笔）"/>
	    <TextView 
	        android:id="@+id/tv_totalMoney"
	        style="@style/textview_pink"
	        android:layout_marginTop="20dp"
	        android:text="总金额：0.00元（0笔）"/>
	    <com.ec2.yspay.widget.ButtonBlueCenter 
	        android:id="@+id/btn_sure"
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
	        android:layout_marginTop="55dp"
	        client:btnContent="打印"/>
	</LinearLayout>
	
</LinearLayout>