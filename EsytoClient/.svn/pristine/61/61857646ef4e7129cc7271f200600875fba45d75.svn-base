<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:client="http://schemas.android.com/apk/res/com.ec2.yspay"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" 
    android:background="@color/white">
	<com.ec2.yspay.widget.MyTitle 
        android:id="@+id/rl_top"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        client:isShowLeft="true"
        client:titleName="支付成功"/>
	<LinearLayout 
        android:id="@+id/pay_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:gravity="center_vertical"
        android:layout_marginTop="20dp">
        <TextView 
            style="@style/textview_black"
            android:textSize="24dp"
            android:text="收款完成"/>
    </LinearLayout>
    <LinearLayout 
        android:id="@+id/ll_ordermsg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:layout_marginLeft="@dimen/margin_left"
        android:layout_marginTop="20dp">
        <LinearLayout 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:orientation="vertical">
	        <TextView 
	            style="@style/textview_gray"
	            android:text="交易号"/>
	        <TextView 
	            style="@style/textview_gray"
	            android:layout_marginTop="@dimen/textgray_margin"
	            android:text="收银员"/>
	        <TextView 
	            style="@style/textview_gray"
	            android:layout_marginTop="@dimen/textgray_margin"
	            android:text="门店"/>
	    </LinearLayout>
	    <LinearLayout 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="15dp"
	        android:orientation="vertical">
	        <TextView 
	            android:id="@+id/tv_orderid"
	            style="@style/textview_gray"
	            android:text=""/>
	        <TextView 
	            android:id="@+id/tv_username"
	            style="@style/textview_gray"
	            android:layout_marginTop="@dimen/textgray_margin"
	            android:text=""/>
	        <TextView 
	            android:id="@+id/tv_shopname"
	            style="@style/textview_gray"
	            android:layout_marginTop="@dimen/textgray_margin"
	            android:text=""/>
	    </LinearLayout>
    </LinearLayout>
    <ImageView 
        android:layout_width="fill_parent"
        android:layout_height="0.5dp"
        android:layout_marginTop="40dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:src="@color/grid_line"/>
    <TextView 
        android:id="@+id/tv_yingshou"
        style="@style/textview_black"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="@dimen/margin_left"
        android:text="应收金额    ￥123.02"/>
    <TextView 
        android:id="@+id/tv_shishou"
        style="@style/textview_black"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="@dimen/margin_left"
        android:text="实收金额    ￥123.02"/>
    <TextView 
        android:id="@+id/tv_zhaoling"
        style="@style/textview_black"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="@dimen/margin_left"
        android:text="找零金额    ￥123.02"/>
	<Button 
	    style="@style/btn_red636_style"
	    android:layout_marginTop="40dp"
	    android:onClick="onclick_back"
	    android:text="完成"/>
	<com.ec2.yspay.widget.ButtonWhiteCenter 
	        android:id="@+id/btn_print"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_gravity="center_horizontal"
	        android:layout_marginTop="10dp"
	        client:btnContent="打印"/>
</LinearLayout>
