<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:client="http://schemas.android.com/apk/res/com.ec2.yspay"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
	<com.ec2.yspay.widget.MyTitle 
        android:id="@+id/rl_top"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        client:isShowLeft="true"
        client:titleName="打印"/>
	<LinearLayout 
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:orientation="vertical"
	    android:padding="@dimen/padding_parent"
	    android:background="@color/white">
	    <TextView 
	        android:id="@+id/tv_orderId"
	        style="@style/textview_black"
	        android:layout_marginTop="@dimen/print_margin_textveiw"
	        android:text="交易流水号："/>
	    <TextView 
	        android:id="@+id/tv_pznum"
	        style="@style/textview_black"
	        android:layout_marginTop="@dimen/print_margin_textveiw"
	        android:text="支付方式："/>
	    <TextView 
	        android:id="@+id/tv_money"
	        style="@style/textview_black"
	        android:layout_marginTop="@dimen/print_margin_textveiw"
	        android:text="金额：0.00元"/>
	    <TextView 
	        android:id="@+id/tv_remark"
	        style="@style/textview_black"
	        android:layout_marginTop="@dimen/print_margin_textveiw"
	        android:text="备注："/>
	    <ImageView
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_marginTop="-10dp"
	        android:layout_gravity="center_horizontal"
	        android:id="@+id/iv_code"/>
	    <com.ec2.yspay.widget.ButtonWhiteCenter
	        android:id="@+id/btn_sure"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_gravity="center_horizontal"
	        android:layout_marginTop="10dp"
	        client:btnContent="打印"/>
	</LinearLayout>
	
</LinearLayout>