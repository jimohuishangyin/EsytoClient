<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MyTitle">
        <attr name="titleName" format="reference|string" />
        <attr name="rightText" format="reference|string" />
        <attr name="srcTitleLeft" format="reference" />
        <attr name="srcTitleRight" format="reference" />
        <attr name="srctitleLeftHead" format = "reference"/>
        <attr name="isShowLeft" format="boolean" />
        <attr name="isShowRight" format="boolean" />
        <attr name="isShowLeftHead" format = "boolean"/>
    </declare-styleable> 
    <declare-styleable name="LlBtn">
        <attr name="btnIcon" format="reference|string" />
        <attr name="btnIconPessed" format="reference|string" />
        <attr name="btnContent" format="string" />
        <attr name="llBackground" format="reference" />
    </declare-styleable>  
    <declare-styleable name="RoundProgressBar">  
        <attr name="roundColor" format="color"/>
        <attr name="roundProgressColor" format="color"/>
        <attr name="roundWidth" format="dimension"></attr>
        <attr name="roundRadius" format="dimension"></attr>
        <attr name="textColor" format="color" />  
        <attr name="textSize" format="dimension" /> 
        <attr name="max" format="integer"></attr> 
        <attr name="textIsDisplayable" format="boolean"></attr>
        <attr name="style">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>
    </declare-styleable> 
</resources>
