<?xml version="1.0" encoding="utf-8"?>
<LinearLayout 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:client="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:paddingLeft="10dp"
    android:paddingTop="12dp"
    android:paddingBottom="12dp"
    android:gravity="center_horizontal"
    android:paddingRight="4dp"
    android:id="@+id/ll_parent">
    <com.ec2.yspay.widget.RoundImageViewNoBorder
            android:id="@+id/iv_logo"
	        android:layout_width="60dp"
	        android:layout_height="60dp"
        	android:src="@drawable/company_default_logo" 
            client:borderRadius="30dp"
            client:type="round" />
    <TextView 
        android:id="@+id/tv_paynum"
        style="@style/textview_black3"
        android:layout_marginTop="5dp"
        android:singleLine="true"
        android:ellipsize="end"
        android:text="现金"/>
</LinearLayout>
